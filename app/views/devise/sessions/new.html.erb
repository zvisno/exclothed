<div class="container col-md-4 col-md-offset-4">

  <%= form_for(resource, :html => {:class => "form-signin"}, as: resource_name, url: session_path(resource_name)) do |f| %>
  <h3 class="form-signin-heading">Sign in</h3>

  <div class="field">
    <%= f.email_field :email, autofocus: true, value: "Email" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "off", value: "Password" %>
  </div>

  <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
  <% end -%>

  <%= f.submit "Sign in", id: "sign_in" %>

  <% end %>
  <%= render "devise/shared/links" %>

</div>



